{
    "name": "uskur/attachments",
    "description": "Attachments plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": ">=3.4 <4.0.0",
        "admad/cakephp-sequence": "^2.0",
        "gumlet/php-image-resize": "^2.0",
        "aws/aws-sdk-php": "^3.314"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Uskur\\Attachments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uskur\\Attachments\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
