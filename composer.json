{
    "name": "uskur/attachments",
    "description": "Attachments plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.1",
        "admad/cakephp-sequence": "^3.0",
        "gumlet/php-image-resize": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Uskur\\Attachments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uskur\\Attachments\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
